{
    "name": "probe-workspace",
    "version": "0.0.1",
    "description": "Solidity debugging tools workspace",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "packageManager": "pnpm@9.0.0",
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "scripts": {
        "build": "pnpm -r run build",
        "dev": "pnpm -r run dev",
        "test": "pnpm -r run test",
        "lint": "pnpm -r run lint",
        "format": "pnpm -r run format",
        "clean": "pnpm -r run clean",
        "typecheck": "pnpm -r run check-types"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/Probe.git"
    },
    "author": "",
    "license": "MIT"
}